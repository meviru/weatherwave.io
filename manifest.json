{
  "manifest_version": 3,
  "name": "Weather Tab",
  "description": "Displays weather information in a new tab",
  "version": "1.0",
  "permissions": ["geolocation"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
